{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "ops_extract text source",
  "type": "object",
  "required": [
    "schema_version",
    "run_id",
    "project",
    "source",
    "entries"
  ],
  "properties": {
    "schema_version": {
      "type": "string",
      "minLength": 1
    },
    "run_id": {
      "type": "string"
    },
    "project": {
      "type": "string"
    },
    "source": {
      "type": "string"
    },
    "entries": {
      "type": "array",
      "items": {
        "type": "object"
      }
    }
  },
  "additionalProperties": true
}
